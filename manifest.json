{
  "manifest_version": 2,
  "name": "Background Extension",
  "version": "1.0",
  "content_scripts": [{
   	"matches": ["<all_urls>"],
   	"js": ["content.js", "jquery-2.1.3.min.js"]
  }],
	"browser_action": {
  	"default_icon": "icon.png"
	},
	"permissions": [
   	"contextMenus", "downloads"
   ],
  "background": {
    "scripts": ["content.js"]
	}
}